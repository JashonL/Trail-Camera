        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/rlv_device"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />


    </com.shuoxd.camera.customview.MySwipeRefreshLayout>


</LinearLayout>